{
    "name": "sampleapp",
    "dockerComposeFile":[
        "docker-compose.yml"
    ],
    "service": "app",
    "workspaceFolder": "/opt/app/src",
    "mounts":[
        "source=${localWorkspaceFolder},target=/opt/app/src,type=bind,consistency=cached"
    ],
    "extensions": [
        "ms-python.python"
    ]
}